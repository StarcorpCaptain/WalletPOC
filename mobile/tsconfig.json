{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@Navigation/*": ["navigation/*"],
      "@Components/*": ["components/*"],
      "@Screens/*": ["screens/*"],
      "@Hooks/*": ["hooks/*"],
      "@Assets/*": ["assets/*"],
      "@Constants/*": ["constants/*"],
      "@Types": ["types"]
    }
  }
}
